  </property><property seq='8' type='text_field'>
    <name>company_contact_name</name>
    <category>Customer Contact Info</category>
    <tooltip>The name of the administrator managing this Avamar server</tooltip>
    <value default=''/>
    <validation>(.*)</validation>
    <displayName>Company contact name</displayName>
    <advanced>no</advanced>
  </property><property seq='9' type='text_field'>
    <name>company_contact_phone_number</name>
    <category>Customer Contact Info</category>
    <tooltip>The phone number of the administrator managing this Avamar server (digits, plus, parenthesis, dash, space - use 'x' for extension)</tooltip>
    <value default=''/>
    <validation>(^$)|(^\S[0-9\s()+-x,]+$)</validation>
    <displayName>Company contact phone number</displayName>
    <advanced>no</advanced>
  </property><property seq='10' type='text_field'>
    <name>company_contact_email_address</name>
    <category>Customer Contact Info</category>
    <tooltip>The email address of the administrator managing this Avamar server</tooltip>
    <value default=''/>
    <validation>(^$)|(^([0-9a-zA-Z]([-\.\w]*[0-9a-zA-Z])*@([0-9a-zA-Z][-\w]*[0-9a-zA-Z]\.)+[a-zA-Z]{2,63})$)</validation>
    <displayName>Company contact email address</displayName>
    <advanced>no</advanced>
  </property><property seq='11' type='text_field'>
    <name>grid_location</name>
    <category>Customer Contact Info</category>
